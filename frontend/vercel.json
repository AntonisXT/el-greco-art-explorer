{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://el-greco-art-explorer.onrender.com/api/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "https://el-greco-art-explorer.onrender.com/auth/$1"
    }
  ],
  "headers": [
    {
      "source": "/(auth|api)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
